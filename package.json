{
	"name": "raita",
	"version": "0.1.5",
	"license": "MIT",
	"author": "sks147",
	"main": "dist/index.js",
	"module": "dist/raita.esm.js",
	"typings": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"analyze": "size-limit --why",
		"build": "dts build",
		"lint": "dts lint src test",
		"lint-fix": "dts lint src test --fix",
		"prepare": "dts build",
		"size": "size-limit",
		"start": "dts watch",
		"test": "dts test",
		"doc": "typedoc src/index.ts src/types.ts"
	},
	"husky": {
		"hooks": {
			"pre-commit": "dts lint"
		}
	},
	"prettier": {
		"printWidth": 80,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5",
		"useTabs": true,
		"tabWidth": 4
	},
	"jest": {
		"testEnvironment": "node",
		"collectCoverage": true,
		"collectCoverageFrom": [
			"./src/**"
		],
		"coverageThreshold": {
			"global": {
				"branches": 80,
				"functions": 80,
				"lines": 80,
				"statements": 80
			}
		}
	},
	"peerDependencies": {},
	"engines": {
		"node": ">=12"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/sks147/raita.git"
	},
	"homepage": "https://github.com/sks147/raita.git",
	"keywords": [
		"utility",
		"utilities",
		"common-utils",
		"helper-utils",
		"helpers"
	],
	"size-limit": [
		{
			"path": "dist/raita.cjs.production.min.js",
			"limit": "10 KB"
		},
		{
			"path": "dist/raita.esm.js",
			"limit": "10 KB"
		}
	],
	"devDependencies": {
		"@size-limit/preset-small-lib": "^8.2.4",
		"@tsconfig/recommended": "^1.0.2",
		"dts-cli": "^1.6.3",
		"husky": "^8.0.3",
		"size-limit": "^8.2.4",
		"tslib": "^2.5.0",
		"typedoc": "^0.23.25",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"mongodb": "5.0.1",
		"node-phonenumber": "1.0.0",
		"rambda": "7.2.1",
		"written-number": "0.11.1"
	}
}
